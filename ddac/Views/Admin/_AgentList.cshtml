@model IEnumerable<ddac.Models.User>

    @{
        ViewBag.Title = "agentList";
    }
    
    <table class=" table table-hover">
        <thead>
            <tr>
                <th>Agent Id</th>
                <th>Username</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var agent in @Model)
    {
        <tr>
            <td>@agent.ID</td>
            <td>@agent.Username</td>
            <td>@agent.Name</td>
        </tr>
}
        </tbody>
    </table>
